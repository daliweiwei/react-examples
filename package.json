    "systemjs-0.21.6": "npm:systemjs@0.21.6",
    "systemjs-plugin-babel": "0.0.25",
    "typescript": "5.0.4"
  },
  "overrides": {
    "plugin-typescript": {
      "typescript": "^5.0.0"
    },
    "react-dom-experimental": {
      "react": "^18.0.0"
    },
    "react-dom-16": {
      "react": "^18.0.0"
    },
    "react-konva": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0"
    },
    "react-native": {
      "react": "^18.1.0"
