  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.mjs --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.mjs --cache --fix",
    "install-experimental": "npm install react@experimental react-dom@experimental",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "start": "node ./server/server.js",
    "validate": "npm run eslint && npm run prettier",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/systemjs": "0.20.6",
    "eslint": "8.41.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "rimraf": "5.0.1"
  },
  "dependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.22.3",
    "@babel/standalone": "7.21.9",
    "@esm-bundle/react": "17.0.2-fix.1",
    "@esm-bundle/react-dom": "17.0.2-fix.0",
    "axios": "1.4.0",
    "core-js": "3.30.2",
    "create-react-class": "15.7.0",
    "esbuild": "0.19.2",
    "formik": "2.2.9",
    "mime-types": "2.1.35",
    "plugin-typescript": "8.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-experimental": "npm:react@experimental",
    "react-16": "npm:react@16.14.0",
    "react-dom": "18.2.0",
    "react-dom-experimental": "npm:react-dom@experimental",
    "react-dom-16": "npm:react-dom@16.14.0",
    "react-hook-form": "7.43.9",
    "react-redux": "8.0.5",
    "react-router-6": "npm:react-router@6.11.2",
    "react-router-dom": "npm:react-router-dom@5.3.4",
    "react-router-dom-6": "npm:react-router-dom@6.11.2",
    "react-spring": "9.7.1",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
    "scheduler": "0.23.0",
