{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Examples for [React](https://react.dev) and [ReactJS.NET](https://reactjs.net/).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
