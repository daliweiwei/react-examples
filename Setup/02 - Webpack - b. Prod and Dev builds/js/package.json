    "html-webpack-plugin": "5.5.0",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0",
    "webpack-merge": "5.8.0"
