{
  "name": "react-eslint-webpack-babel-vscode",
  "description": "Using React with ESLint, Webpack & Babel (Visual Studio Code)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-transform-react-jsx": "7.12.7",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.0.3",
    "core-js": "3.8.0",
    "css-loader": "4.2.1",
    "eslint": "7.18.0",
    "eslint-webpack-plugin": "2.4.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "next",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "build": "npm run clean && webpack --progress",
