{
  "name": "react-eslint-webpack-babel-vscode",
  "description": "Using React with ESLint, Webpack & Babel (Visual Studio Code)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
