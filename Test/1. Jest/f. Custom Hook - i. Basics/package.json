{
  "name": "react-test-jest-custom-hook-basics",
  "description": "React Tests with Jest - Custom Hook - Basics",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.0",
    "babel-jest": "27.0.2",
    "babel-loader": "8.2.2",
    "core-js": "3.15.0",
    "css-loader": "5.2.6",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.29.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.5.4",
    "html-webpack-plugin": "5.3.1",
    "jest": "27.0.4",
    "react-test-renderer": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "webpack": "5.39.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "scripts": {
    "build": "npm run clean && webpack --progress",
    "clean": "rimraf build",
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "start": "webpack serve --open",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch"
  }
}
